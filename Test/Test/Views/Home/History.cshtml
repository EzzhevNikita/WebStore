@model IEnumerable<Test.Models.HistoryViewModel>
@{
    ViewBag.Title = "History";
}

<h2>History</h2>

<!DOCTYPE html>

<html>
<head Products />
<body>
    <div class="items">
        @Html.ValidationMessage("Error")
        @if (Model != null)
        {
            <table>
                <tr>
                    <th style="border: 1px solid black">Name</th>
                    <th style="border: 1px solid black">Description</th>
                    <th style="border: 1px solid black">Date of buy</th>
                    <th style="border: 1px solid black">Price</th>
                </tr>
                @foreach (var p in Model)
                {
                    <tr>
                        <th style="border: 1px solid black">@Html.DisplayFor(modelItem => p.Name)</th>
                        <th style="border: 1px solid black">@Html.DisplayFor(modelItem => p.Descrption)</th>
                        <th style="border: 1px solid black">@Html.DisplayFor(modelItem => p.Date_of_buy)</th>
                        <th style="border: 1px solid black">@Html.DisplayFor(modelItem => p.Price)</th>
                    </tr>
                }
            </table>
        }
    </div>
</body>
</html>
